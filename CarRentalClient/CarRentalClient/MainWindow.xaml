<Window x:Class="CarRentalClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CarRentalClient"
        mc:Ignorable="d"
        Title="Wypożyczalnia samochodów" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>

        <Button Name="AddCar" Content="Dodaj samochód"  Grid.Row="0" Click="AddCar_Click"></Button>
        <Button Name="RentCar" Content="Wypożycz samochód" Grid.Row="1" Click="RentCar_Click"></Button>
        <Button Name="Exit" Content="Wyjście" Grid.Row="2" Click="Exit_Click"></Button>
    </Grid>
</Window>
